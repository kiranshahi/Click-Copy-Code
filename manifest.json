{
  "name": "Click Copy {Code}",
  "version": "1.0",
  "author": "Kiran Shahi",
  "homepage_url": null,
  "description": "Software engineer's friend. Helps to copy code from stackoverflow in double click.",
  "icons": {
    "16": "images/curly-brackets-16.png",
    "32": "images/curly-brackets-32.png",
    "48": "images/curly-brackets-64.png",
    "128": "images/curly-brackets-128.png"
  },
  "content_scripts": [
    {
      "matches": [ "<all_urls>" ],
      "js": [ "script.js" ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}